                        //console.log('width-'+videowidth, 'height-'+videoheight);
                        if(videowidth<videoheight){
                            //console.log('if');
                            $(this).parents('.mejs__container').addClass('postVideoHeight');
                        }
                        $(this).parents('.mejs__container').css('opacity','1');
                    });
                }, 600);
            });




