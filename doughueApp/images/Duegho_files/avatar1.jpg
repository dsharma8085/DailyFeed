                // 	setTimeout(function(){
                // 		$('.viewerloading').fadeOut(600);
                // 	},600);
                // });
                $('#photoViewer').on('hidden.bs.modal', function (e) {
                    var $owl = $('#photoViewer-slider').owlCarousel();
                    $owl.trigger('destroy.owl.carousel');
                    $('.viewerloading').fadeIn();
                });
            });
            $(window).on('load', function(){
                $('.postbar-row').find('.mejs__container').css('opacity','0');
                setTimeout(() => {
                    $('.postbar-row').find('video').each(function(){
